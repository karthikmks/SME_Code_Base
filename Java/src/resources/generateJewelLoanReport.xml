		<sqlquery>select a.jewel_loan_no, a.jewel_loan_transaction_date, d.jewel_loan_type, e.jewel_take_over_from, c.jewel_scheme_name,
		a.rate_per_gram, a.loan_amount, b.customer_name, b.address,b.mobile_no, a.document_charges, a.is_this_ledger_loan, a.interest_rate, 
		a.service_charges, a.total_gross_weight, a.total_reduced_weight, a.total_net_weight, a.total_count, a.created_by, a.created_timestamp
		from jewel_loan_transaction a, customer b, jewel_scheme c, jewel_loan_type d, jewel_take_over_from e
		where a.active_flag in (?) and a.customer_id = b.customer_id and d.jewel_loan_type_id = a.jewel_loan_type_id and 
		c.jewel_scheme_id = a.jewel_scheme_id and e.jewel_take_over_from_id = a.jewel_take_over_from_id and 
		date(jewel_loan_transaction_date) between date(?) and date(?)</sqlquery>
