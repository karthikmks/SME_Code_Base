<storedprocedures>
	<storedprocedure code="1" name="sql">
		<sqlquery>select a.loan_no, a.payment_date,c.payment_type,a.actual_interest_rate,a.no_of_days,
		a.principal_amount, a.interest_amount, a.customer_interest_rate, a.created_by, a.created_timestamp    
		from payment_transaction a, deposit_loan_transaction b, payment_type c where date(a.payment_date) 
		between date(?) and date(?) and a.loan_no = b.deposit_loan_no and b.active_flag in (?)
		and a.transaction_type_id = 2 and a.payment_type_id = c.payment_type_id</sqlquery>		
